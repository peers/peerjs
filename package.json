  "types": "./index.d.ts",
  "devDependencies": {
    "@types/chai": "^4.2.2",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "jsdom": "^15.1.1",
    "jsdom-global": "^3.0.2",
    "mocha": "^6.2.0",
    "mock-socket": "8.0.5",
    "parcel-bundler": "^1.12.4",
    "standard": "^14.1.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "@types/node": "^10.14.17",
    "@types/webrtc": "^0.0.25",
    "eventemitter3": "^3.1.2",
    "opencollective": "^1.0.3",
    "opencollective-postinstall": "^2.0.0",
    "peerjs-js-binarypack": "1.0.1",
    "webrtc-adapter": "^7.3.0"
